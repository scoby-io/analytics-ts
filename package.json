{
  "name": "@scoby/analytics-ts",
  "version": "2.1.0",
  "private": false,
  "description": "scoby's official Typescript client to measure web traffic",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/jest": "^29.5.1",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "nock": "^13.3.0",
    "prettier": "^2.8.8",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "typedoc": "^0.24.5",
    "typedoc-plugin-markdown": "^3.15.2",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "doc": "typedoc src/index.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint \"*/**/*.{ts,js,json}\"",
    "lint:fix": "eslint \"*/**/*.{ts,js,json}\" --fix",
    "build": "rollup --config ./rollup.config.js",
    "release": "standard-version --tag-prefix \"\" --dry-run",
    "pre-release": "npm run doc && npm run lint && npm run test:coverage && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scobyio/analytics-ts.git"
  },
  "keywords": [],
  "author": {
    "name": "Scoby UG"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scobyio/analytics-ts/issues"
  },
  "homepage": "https://github.com/scobyio/analytics-ts#readme",
  "dependencies": {
    "build-url-ts": "6.1.5",
    "got": "11.8.5",
    "ip-matching": "2.1.2"
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}} [skip ci]"
  }
}
